<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.app1" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Power Monitor</name>
    <description>
        Skripsi Andi
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access origin="*" />
    <access origin="http://andi.rf.gd/*" />
    <allow-navigation href="*://andi.rf.gd/*" />
    <access origin="*" subdomains="true" />
    <platform name="android">
        <preference name="android-targetSdkVersion" value="26" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="es6-promise-plugin" spec="~4.1.0" />
    <plugin name="cordova-plugin-android-wifi-manager" spec="~1.0.0" />
    <plugin name="cordova-plugin-http" spec="^1.2.0" />
    <plugin name="cordovanetworkmanager" spec="^2.6.0" />
    <plugin name="cordova-plugin-restful" spec="0.0.4" />
    <engine name="android" spec="^7.0.0" />
    <engine name="browser" spec="^5.0.4" />
</widget>
