<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.app1" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Power Monitor</name>
    <description>
        Skripsi Andi
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <allow-intent href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access origin="*" />
    <access origin="http://192.168.2.98/*" />
    <allow-navigation href="*://192.168.2.98/*" />
    <access origin="*" subdomains="true" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
